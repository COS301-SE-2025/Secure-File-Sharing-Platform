{
  "name": "sfsp-admin",
  "private": true,
  "version": "0.0.0",
  "description": "Admin desktop app for SecureShare",
  "author": "CacheME",
  "type": "module",
  "main": "src/electron/main.js",
  "scripts": {
    "clean": "rimraf dist dist-react",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:5123 && cross-env NODE_ENV=development electron .",
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist:mac": "npm run clean && npm run build && electron-builder --mac --arm64",
    "dist:win": "npm run clean && npm run build && electron-builder --win --x64",
    "dist:linux": "npm run clean && npm run build && electron-builder --linux --x64"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^10.0.0",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  }
}
