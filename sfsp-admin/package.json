{
  "name": "sfsp-admin",
  "private": true,
  "version": "0.0.0",
  "description": "Admin desktop app for SecureShare",
  "author": "CacheME",
  "type": "module",
  "main": "src/electron/main.js",
  "scripts": {
    "clean": "rimraf dist dist-react",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:5123 && cross-env NODE_ENV=development electron .",
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist:mac": "npm run clean && npm run build && electron-builder --mac --arm64",
    "dist:win": "npm run clean && npm run build && electron-builder --win --x64",
    "dist:linux": "npm run clean && npm run build && electron-builder --linux --x64",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "lucide-react": "^0.544.0",
    "ogl": "^1.0.11",
    "os-utils": "^0.0.14",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.0.0",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-transform-stub": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  }
}
